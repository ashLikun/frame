<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SuperToolBar">
        <attr name="stb_titleColor" format="color" />
        <attr name="stb_titleSize" format="dimension" />
        <attr name="stb_title" format="string" />
        <attr name="stb_backImgColor" format="color" />
        <!--返回键是否是正方形-->
        <attr name="stb_backImgSquare" format="boolean" />
        <!--返回键的内间距-->
        <attr name="stb_backImgPadding" format="boolean" />
        <attr name="stb_bottonLineColor" format="color" />
        <attr name="stb_actionTextColor" format="color" />
        <attr name="stb_bottonLineHeight" format="dimension" />
        <attr name="stb_backImg" format="reference" />
        <attr name="stb_notificationBagColor" format="color" />
        <attr name="stb_notificationTextColor" format="color" />
        <attr name="stb_notificationStrokeColor" format="color" />
        <attr name="stb_androidMTranslucentStatusBarColor" format="color" />
        <attr name="stb_actionPadding" format="dimension" />
        <attr name="stb_actionWidth" format="dimension" />
        <attr name="stb_actionHeight" format="dimension" />
    </declare-styleable>
</resources>