<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlatButton">
        <attr name="android:background" />
        <attr name="android:textColor" />

        <attr name="colorPressed" format="color" />
        <attr name="colorPressed2" format="color" />
        <attr name="colorPressed3" format="color" />
        <attr name="colorFocused" format="color" />
        <attr name="colorFocused2" format="color" />
        <attr name="colorFocused3" format="color" />
        <attr name="colorSelected" format="color" />
        <attr name="colorSelected2" format="color" />
        <attr name="colorSelected3" format="color" />
        <attr name="colorNormal" format="color" />
        <attr name="colorNormal2" format="color" />
        <attr name="colorNormal3" format="color" />
        <attr name="colorEnable" format="color" />
        <attr name="colorEnable2" format="color" />
        <attr name="colorEnable3" format="color" />
        <attr name="colorPressedText" format="color" />
        <attr name="colorEnableText" format="color" />
        <attr name="colorRipple" format="color" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeColorSelect" format="color" />
        <attr name="strokeColorEnable" format="color" />
        <attr name="strokeColorFocused" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="cornerRadius" format="dimension" />
        <attr name="clickDelay" format="integer" />
        <attr name="isUseTextColorList" format="boolean" />


        <attr name="colorNormalOrientation" format="enum">
            <enum name="TOP_BOTTOM" value="0"></enum>
            <enum name="TR_BL" value="1"></enum>
            <enum name="RIGHT_LEFT" value="2"></enum>
            <enum name="BR_TL" value="3"></enum>
            <enum name="BOTTOM_TOP" value="4"></enum>
            <enum name="BL_TR" value="5"></enum>
            <enum name="LEFT_RIGHT" value="6"></enum>
            <enum name="TL_BR" value="7"></enum>
        </attr>
        <attr name="colorPressedOrientation" format="enum">
            <enum name="TOP_BOTTOM" value="0"></enum>
            <enum name="TR_BL" value="1"></enum>
            <enum name="RIGHT_LEFT" value="2"></enum>
            <enum name="BR_TL" value="3"></enum>
            <enum name="BOTTOM_TOP" value="4"></enum>
            <enum name="BL_TR" value="5"></enum>
            <enum name="LEFT_RIGHT" value="6"></enum>
            <enum name="TL_BR" value="7"></enum>
        </attr>
        <attr name="colorEnableOrientation" format="enum">
            <enum name="TOP_BOTTOM" value="0"></enum>
            <enum name="TR_BL" value="1"></enum>
            <enum name="RIGHT_LEFT" value="2"></enum>
            <enum name="BR_TL" value="3"></enum>
            <enum name="BOTTOM_TOP" value="4"></enum>
            <enum name="BL_TR" value="5"></enum>
            <enum name="LEFT_RIGHT" value="6"></enum>
            <enum name="TL_BR" value="7"></enum>
